<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dao.operatorDao.Operator" table="OPERATOR" schema="YARA">
        <id name="operatorid" type="java.lang.Integer">
            <column name="OPERATORID" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="loginname" type="java.lang.String">
            <column name="LOGINNAME" length="20" not-null="true" />
        </property>
        <property name="password" type="java.lang.String">
            <column name="PASSWORD" length="20" not-null="true" />
        </property>
        <property name="empid" type="java.lang.Integer">
            <column name="EMPID" precision="5" scale="0" />
        </property>
    </class>
</hibernate-mapping>
